<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sms.within30" >
<uses-sdk
    android:minSdkVersion="17"
    android:targetSdkVersion="23"/>
    <!--android:theme="@style/AppTheme"-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/NoActionBarTheme.Light" >
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" ></activity>
        <activity
            android:name=".LandingActivity"
            android:label="@string/app_name" ></activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" ></activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/app_name" ></activity>
        <activity
        android:name=".CategoryItemsActivity"
        android:label="@string/app_name" ></activity>
        <activity
            android:name=".CategoryItemDetailsActivity"
            android:label="@string/app_name" ></activity>
        <activity
            android:name="com.sms.within30.calender.BookingTimeSelectionActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden"></activity>




    </application>

</manifest>
